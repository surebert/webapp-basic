{
    "require": {
        "surebert/toolkit": "4.9977",
        "surebert/sbf": "1.0"
    },
    "autoload": {
        "psr-0": {
            "Models" : ["private"],
            "Controllers" : ["private"],
            "Traits" : ["private"]
    }
    },
    "repositories": [

        {
            "type": "package",
            "package": {
                "name": "surebert/toolkit",
                "version": "4.9977",
                "source": {
                    "url": "https://github.com/surebert/surebert-toolkit.git",
                    "type": "git",
                    "reference": "4.9977"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "surebert/sbf",
                "version": "1.0",
                "autoload": {
                    "psr-0": {
                        "sb": "src/"
                    }
                },
                "source": {
                    "url": "https://github.com/surebert/surebert-framework.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ]

}